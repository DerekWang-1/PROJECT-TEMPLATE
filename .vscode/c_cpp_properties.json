{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}"
        ]
    },
    "configurations": [
        {
            "name": "xv6",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "cStandard": "c11",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4
}